package com.bingfeng.abstructfactory;

import java.sql.DriverManager;

public class Test {

	
	/*
	 * 抽象工厂模式相对于工厂模式区别在于创建的是一个产品族
	 */
	public static void main(String[] args) {
		
		try {
			Class clazz = Class.forName("");
			clazz.newInstance();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
}
